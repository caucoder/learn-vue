st('https://caucoder.firebaseio.com/posts.json', this.blog).then(function(data){\n                console.log(data);\n                this.submitted = true;\n            });\n          }\n  }\n\n}\n</script>\n\n<style scoped>\n#add-blog *{\n    box-sizing: border-box;\n}\n#add-blog{\n    margin: 20px auto;\n    max-width: 500px;\n }\n\nlabel{\n    display: block;\n    margin: 20px 0 10px;\n}\ninput[type=\"text\"], textarea{\n    display: block;\n    width: 100%;\n    padding: 8px;\n}\n \n#preview{\n    padding: 10px 20px;\n    border: 1px dotted #ccc;\n    margin: 30px 0;\n}\nh3{\n    margin-top: 10px;\n}\n\n#checkboxes input{\n    display: inline-block;\n    margin-right: 10px;\n}\n\n\n#checkboxes label{\n    display: inline-block;\n}\n</style>\n"]}]}